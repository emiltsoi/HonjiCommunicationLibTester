        <TextBox x:Name="MFC2In" HorizontalAlignment="Left" Height="23" Margin="260,501,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="DurationIn" HorizontalAlignment="Left" Height="23" Margin="349,501,0,0" TextWrapping="Wrap" Text="60" VerticalAlignment="Top" Width="81"></TextBox>
