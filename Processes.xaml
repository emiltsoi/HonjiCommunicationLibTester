        <Label Content="ARC1" HorizontalAlignment="Left" Margin="345,331,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Content="ARC2" HorizontalAlignment="Left" Margin="440,330,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Content="ARC3" HorizontalAlignment="Left" Margin="526,330,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Content="ARC4" HorizontalAlignment="Left" Margin="619,331,0,0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="Arc5In" HorizontalAlignment="Left" Height="23" Margin="170,430,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="81" Text="0"/>
        <TextBox x:Name="Arc6In" HorizontalAlignment="Left" Height="23" Margin="260,430,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="BiasIn" HorizontalAlignment="Left" Height="23" Margin="349,430,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="Heater1In" HorizontalAlignment="Left" Height="23" Margin="439,430,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="Heater2In" HorizontalAlignment="Left" Height="23" Margin="526,430,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="Heater3In" HorizontalAlignment="Left" Height="23" Margin="619,430,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="81"/>
