        <TextBox x:Name="PFError" HorizontalAlignment="Left" Height="23" Margin="525,215,0,0" TextWrapping="Wrap" Text="Unknown" VerticalAlignment="Top" Width="220"/>
        <CheckBox x:Name="CanRunStep" Content="CanRunRecipeStep" HorizontalAlignment="Left" Margin="170,265,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.908,-9.267"/>
        <Button x:Name="RunStep" Content="RunStep" HorizontalAlignment="Left" Margin="345,262,0,0" VerticalAlignment="Top" Width="75" Click="RunStep_Click"/>
